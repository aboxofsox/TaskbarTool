<Window x:Class="TaskbarTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaskbarTool"
        mc:Ignorable="d"
        Title="Taskbar Tool" Height="200" Width="400">
    <Grid>
        <TextBlock Text="Taskbar Tool" Margin="0,31,0,0" FontWeight="Bold" FontSize="18" Width="114" Height="27" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <ComboBox Height="20" Margin="0,72,0,0" x:Name="PositionBox" Width="140" HorizontalAlignment="Center" VerticalAlignment="Top" />
        <Button Height="30" Content="Set" Margin="0,117,0,0" x:Name="SetTaskBar" Click="SetTaskBar_Click" Width="88" HorizontalAlignment="Center" VerticalAlignment="Top"/>

        <TextBlock Width="120" Height="20" Margin="656,8,24,406" x:Name="TestKey"/>
    </Grid>
</Window>
